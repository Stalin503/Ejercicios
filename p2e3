package periodo2;

import java.util.Scanner;
public class p2e3 {

    public static void main(String[] args) {
        Scanner InS = new Scanner (System.in);
        int fact=1;
        int num=0;
        System.out.print("Introduce numero");
        num=InS.nextInt();
        
        for(int ind=1; ind<=num; ind++)
        {
            fact*=ind;
        }  
        System.out.print("El factorial es: "+fact);
    }    
}
