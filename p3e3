package periodo3;

import java.util.Scanner;
public class p3e3 {

    public static void main(String[] args) {
        Scanner SuS = new Scanner (System.in);
        int fact=1,n, cont=1;
        
        System.out.print("Ingrese un n√∫mero:");
        n= SuS.nextInt(); 
       
        do {
            fact=fact*cont;
            cont=cont+1;
        }
        while (cont<=n);
        System.out.print("El factorial es "+fact);
    }
}
